{
  "name": "milan-app",
  "version": "0.3.7",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/user-event": "^13.5.0",
    "aos": "^2.3.4",
    "axios": "^0.26.0",
    "cypress": "^10.8.0",
    "dotenv": "^16.0.3",
    "js-cookie": "^3.0.1",
    "jsonschema": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-icons": "^4.7.1",
    "react-js-loader": "^0.1.0",
    "react-router-dom": "^6.4.3",
    "react-scripts": "^2.1.3",
    "react-spinners": "^0.13.7",
    "react-toastify": "^9.1.1",
    "swr": "^2.1.0",
    "url": "^0.11.0",
    "vite-plugin-svgr": "^2.2.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "prepare": "husky install",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix && npm format",
    "format": "prettier --write src"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint . --fix",
      "prettier --write"
    ],
    "src/**/*.html": [
      "prettier --write"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^2.2.0",
    "eslint-plugin-cypress": "^2.12.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.2",
    "vite": "^3.2.3"
  }
}
